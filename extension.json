{
	"name": "MwOAuth2Client",
	"version": "0.0.3",
	"author": [
		"Stefan2904 & Kasper Rynning-TÃ¸nnesen"
	],
	"requires": {
		"MediaWiki": ">= 1.26.0"
	},
	"url": "https://github.com/flytreeleft/MediaWiki-OAuth2-Client",
	"descriptionmsg": "OAuth2 extension for MediaWiki to integrate OAuth2 server (Keycloak, github, etc.) as an identity provider.",
	"license-name": "MIT",
	"type": "auth",
	"AutoloadClasses": {
		"OAuth2ClientHooks": "OAuth2Client.hooks.php",
		"SpecialOAuth2Client": "SpecialOAuth2Client.php",
		"OAuth2":"OAuth2-Client/OAuth2Client.php"
	},
	"Hooks": {
		"UserLogout": [
			"OAuth2ClientHooks::onUserLogout"
		],
		"UserLoginForm": [
			"OAuth2ClientHooks::onUserLoginForm"
		],
		"BeforePageDisplay": [
			"OAuth2ClientHooks::onBeforePageDisplay"
		],
		"LoadExtensionSchemaUpdates": [
			"OAuth2ClientHooks::onLoadExtensionSchemaUpdates"
		]
	},
	"ResourceFileModulePaths": {
		"localBasePath": "",
		"remoteExtPath": "BoilerPlate"
	},
	"SpecialPages": {
		"OAuth2Client": "SpecialOAuth2Client"
	},
	"manifest_version": 1
}
